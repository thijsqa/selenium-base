{
  "capabilities": [
    {
      "browserName": "*firefox",
      "maxInstances": 1,
      "seleniumProtocol": "Selenium"
    },
    {
      "browserName": "firefox",
      "maxInstances": 2,
      "seleniumProtocol": "WebDriver"
    },
	{
	  "browserName": "htmlunit",
	  "maxInstances": 1,
	  "seleniumProtocol": "WebDriver"
	}
  ],
  "configuration": {
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "maxSession": 1,
	
	"nodeTimeout":120,
    "port": 5555,
	
    "register": true,
    "registerCycle":10000,
	"cleanUpCycle":2000,
	
	"newSessionWaitTimeout":60000
  }
}
